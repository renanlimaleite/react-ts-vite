{
  "name": "react-ts-vite",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@vitejs/plugin-react": "^1.3.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "lint-staged": "^13.0.1",
    "prettier": "^2.6.2",
    "typescript": "^4.6.3",
    "vite": "^2.9.9",
    "husky": "^8.0.0"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn tsc",
      "yarn eslint --fix"
    ],
    "src/**/**/**/*.{ts,tsx}": [
      "yarn tsc",
      "yarn eslint --fix"
    ]
  }
